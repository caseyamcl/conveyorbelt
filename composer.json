{
  "name": "caseyamcl/shuttle",
  "description": "Data Migration Tool - Get Data from Here to There",
  "require": {
    "php"                     : ">=5.4",
    "caseyamcl/tasktracker"   : "~2.0",
    "symfony/yaml"            : "~2.6",
    "doctrine/dbal"           : "~2.5",
    "symfony/event-dispatcher": "~2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5"
  },
  "autoload": {
    "psr-4": {
      "Shuttle\\": "src/",
      "ShuttleMigrator\\": "migrators/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ShuttleTest\\": "tests/"
    }
  }
}
